QLineEdit {
  color: #fff;
  background: #2b2b2b; 
  border-radius: 10px;
  font-family: 'Montserrat', sans-serif;
  font-size: 18pt;
  padding: 8px;
}
QListView
{
   border: 2px solid black;
   outline: 0;
   background-color: white;
   color: black;
}

 QListView::item
{
   padding-left: 5px;
}

 QListView::item:active
{
   background-color: grey;
}

 QListView::item:hover
{
   background-color: lightgray;
}